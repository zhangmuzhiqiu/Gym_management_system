<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.Gym_management_system.mapper.CourseMapper">

    <insert id="addCourse" parameterType="com.example.Gym_management_system.entity.Course"
            useGeneratedKeys="true" keyProperty="id">
        INSERT INTO `c_course` (courseName, coursePrice, trainerId)
        VALUES (#{courseName}, #{coursePrice}, #{trainerId})
    </insert>
</mapper>
